    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Litestar app",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "justMyCode": true,
            "args": [
                "app:app",
                "--port",
                "9999",
                "--reload",
                "--log-level",
                "debug"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "type": "chrome",
            "name": "http://127.0.0.1:3000/templates/index2.html",
            "request": "launch",
            "url": "http://127.0.0.1:3000/templates/index2.html"
        },
        {
            "name": "Python: Litestar D",
            "type": "debugpy",
            "request": "launch",
            "module": "litestar",
            "justMyCode": true,
            "args": ["run" ,"--reload","--port","9999"]
        }
    ]
}